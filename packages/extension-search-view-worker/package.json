{
  "name": "@lvce-editor/extension-search-view",
  "version": "0.0.0-dev",
  "description": "Extension Search View Worker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/extension-search-view.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/extensionSearchViewWorkerMain.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@lvce-editor/assert": "^1.5.1",
    "@lvce-editor/constants": "^1.42.0",
    "@lvce-editor/i18n": "^2.1.0",
    "@lvce-editor/list": "^1.7.0",
    "@lvce-editor/rpc": "^4.20.0",
    "@lvce-editor/rpc-registry": "^5.3.0",
    "@lvce-editor/verror": "^1.7.0",
    "@lvce-editor/viewlet-registry": "^2.0.0",
    "@lvce-editor/virtual-dom-worker": "^4.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
